<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.accounting.mapper.PostingMapper">
    
    <select id="selectByEntryId" resultType="com.accounting.entity.Posting">
        SELECT * FROM posting WHERE entry_id = #{entryId}
